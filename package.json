{
  "name": "@wunderwa/stamdic",
  "version": "0.2.0",
  "description": "Stamdic - static site generator with pug, bootstrap, sass, markdown",
  "type": "module",
  "engines": {
    "node": ">= 22.12 < 23"
  },
  "devDependencies": {
    "@eslint/js": "~9.10",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/bootstrap": "^5.2.10",
    "@types/eslint__js": "~8.42",
    "@types/gulp": "^4.0.17",
    "@types/gulp-autoprefixer": "^0.0.37",
    "@types/gulp-babel": "^6.1.33",
    "@types/gulp-dart-sass": "^1.0.5",
    "@types/gulp-rename": "^2.0.6",
    "@types/lodash": "^4.17.15",
    "@types/markdown-it": "^14.1.2",
    "@types/markdown-it-attrs": "^4.1.3",
    "@types/minimist": "^1.2.5",
    "@types/node": "^22.10.2",
    "@types/pug": "^2.0.10",
    "@typescript-eslint/parser": "~8.5",
    "@vitest/coverage-v8": "~2.1",
    "@vitest/eslint-plugin": "~1.1",
    "babel-preset-env": "^1.7.0",
    "color-json": "^3.0.5",
    "eslint": "~9.10",
    "eslint-config-prettier": "~9.1",
    "globals": "~15.9",
    "gulp": "^5.0.0",
    "gulp-autoprefixer": "^9.0.0",
    "gulp-dart-sass": "^1.1.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.1.4",
    "gulp-typescript": "5.0.1",
    "gulp-webp": "^5.0.0",
    "lodash": "^4.17.21",
    "markdown-it": "^14.1.0",
    "markdown-it-attrs": "^4.3.1",
    "minimist": "^1.2.8",
    "parcel": "^2.13.3",
    "parcel-bundler": "^1.12.5",
    "prettier": "~3.3",
    "pug": "^3.0.3",
    "rollup": "^4.30.1",
    "ts-api-utils": "^1.4.3",
    "tslib": "^2.8.1",
    "tsx": "^4.19.2",
    "typescript": "~5.5.2",
    "typescript-eslint": "~8.5",
    "vitest": "~2.1",
    "yaml": "^2.6.1"
  },
  "scripts": {
    "man": " ./actions/man",
    "fmt": "prettier \"{src,__{tests}__}/**/*.{ts,mts,json}\" --config .prettierrc --write",
    "fmt:site": "prettier \"{sites/$1,__{tests}__}/**/*.{ts,mts,json}\" --config .prettierrc --write",
    "fmt:check": "prettier \"{src,__{tests}__}/**/*.{ts,mts,json}\" --config .prettierrc --check",
    "doc": " actions/doc $@",
    "copy": "./actions/copy $@",
    "srv": "./actions/server $@",
    "logo": "actions/logo",
    "new": "node --import=tsx src/new.ts $@",
    "vars": "node --import=tsx ./src/vars.ts $@",
    "wrk": "./wrk $@",
    "prod": "./wrk -bd $1",
    "-": "--- below: unused ",
    "prebuild": "npm run lint",
    "lint": "eslint .",
    "test": "vitest run unit",
    "test:coverage": "vitest run unit --coverage.enabled --coverage.all",
    "test:watch": "vitest unit"
  },
  "author": "Andrei Zagoruiko<adkrix@gmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "bootstrap": "^5.3.3"
  },
  "volta": {
    "node": "22.12.0"
  },
  "keywords": [
    "ssg",
    "node",
    "tsx",
    "typescript",
    "pug",
    "markdown",
    "yaml",
    "sass",
    "scss",
    "static",
    "site",
    "generator",
    "deploy"
  ]
}
